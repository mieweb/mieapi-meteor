{
  "name": "meteor-app",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "test": "meteor test --once --driver-package meteortesting:mocha",
    "test-app": "TEST_WATCH=1 meteor test --full-app --driver-package meteortesting:mocha",
    "visualize": "meteor --production --extra-packages bundle-visualizer",
    "build:css": "postcss client/styles/tailwind.css -o public/tailwind.css"
  },
  "dependencies": {
    "@babel/runtime": "^7.20.7",
    "aws-sdk": "^2.1691.0",
    "base-64": "^1.0.0",
    "bcrypt": "^5.1.1",
    "fetch-blob": "^4.0.0",
    "meteor-node-stubs": "^1.2.10",
    "mock-aws-s3": "^4.0.2",
    "nock": "^13.5.5",
    "node-fetch": "^3.3.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.26.2",
    "tailwindcss": "^3.4.13",
    "winston": "^3.14.2"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.jsx",
      "server": "server/main.js"
    },
    "testModule": "tests/main.js"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.47",
    "postcss-cli": "^11.0.0"
  }
}
